{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "headers": [
      {
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache"
          }
        ],
        "source": "/service-worker.js"
      }
    ],
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "public": "build"
  },
  "storage": {
    "rules": "storage.rules"
  }
}
